{
  "name": "croquet-australia-admin-website",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "build": "ng build -prod",
    "init": "npm install --global --no-optional ts-node typescript@2 && npm install --no-optional chalk shelljs @types/chalk @types/shelljs && ts-node scripts/npm/init.ts",
    "start": "ng serve",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test --watch false && ng e2e",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "appveyor:install": "npm run init && npm install --no-optional",
    "appveyor:build": "npm run build && ts-node scripts/appveyor/configure-iis",
    "appveyor:test": "npm test",
    "azure:create-website": "ts-node scripts/azure/create-website"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.0.0",
    "@angular/compiler": "2.0.0",
    "@angular/core": "2.0.0",
    "@angular/forms": "2.0.0",
    "@angular/http": "2.0.0",
    "@angular/platform-browser": "2.0.0",
    "@angular/platform-browser-dynamic": "2.0.0",
    "@angular/router": "3.0.0",
    "core-js": "^2.4.1",
    "rxjs": "5.0.0-beta.12",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "@types/chalk": "^0.4.29",
    "@types/jasmine": "^2.2.30",
    "@types/node": "^6.0.39",
    "@types/shelljs": "^0.3.31",
    "angular-cli": "^1.0.0-beta.15",
    "azure-mgmt-website": "^0.9.16",
    "chalk": "^1.1.3",
    "codelyzer": "~0.0.26",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.2.1",
    "protractor": "4.0.5",
    "ts-node": "1.2.1",
    "tslint": "3.13.0",
    "typescript": "2.0.2"
  }
}
